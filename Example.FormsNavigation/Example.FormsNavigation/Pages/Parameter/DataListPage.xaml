<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:pages="clr-namespace:Example.FormsNavigation.Pages.Parameter;assembly=Example.FormsNavigation"
             mc:Ignorable="d"
             d:DataContext="{d:DesignInstance Type={x:Type pages:DataListPageViewModel}}"
             x:Class="Example.FormsNavigation.Pages.Parameter.DataListPage"
             NavigationPage.HasNavigationBar="False">

  <StackLayout VerticalOptions="Start">
    <Button Text="Back" Command="{Binding BackCommand}" CommandParameter="/MenuPage" />
  </StackLayout>

</ContentPage>